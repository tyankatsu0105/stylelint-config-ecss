{
  "name": "stylelint-config-ecss",
  "version": "0.1.1",
  "description": "A stylelint config based on ECSS",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "stylelint-ecss",
    "ecss"
  ],
  "homepage": "https://github.com/tyankatsu0105/stylelint-config-ecss",
  "bugs": {
    "url": "https://github.com/tyankatsu0105/stylelint-config-ecss/issues"
  },
  "license": "MIT",
  "author": "tyankatsu <frips.ryilsufupe+dev@gmail.com>",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "repository": "git@github.com:tyankatsu0105/stylelint-config-ecss.git",
  "scripts": {
    "test": "stylelint tests/fixture/**/*.vue --config tests/.stylelintrc.js",
    "test:watch": "onchange -d 1000 'tests/fixture/**/*.vue' -- yarn stylelint 'tests/fixture/**/*.vue' --config tests/.stylelintrc.js",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "commit": "git-cz"
  },
  "dependencies": {
    "stylelint-plugin-ecss": "^1.0.0"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "lint-staged": "^10.2.2",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "stylelint": "^13.3.3",
    "stylelint-declaration-strict-value": "^1.4.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
